#!/bin/csh

# Build libraries first.

foreach file ( hdr_api lib* vardb/lib )
  (cd $file; make install)
end


# Programs with "install" target.

foreach file ( hdrbld ncplot ncpp/src vardb/editor xhd xpms2d/src )
  (cd $file; make install)
end


# Programs with Qt programs

foreach file ( rtplot ads_utils/hvpsdiag )
  (cd $file; qmake; make)
end


# Programs with "install" target.

cd nc_utils
foreach file ( asc2cdf n2asc ncReorder ncav ncextr ncmerge )
  (cd $file; make install)
end
cd ..
