\begin_inset Newline newline
\end_inset


\begin_inset Formula \begin{eqnarray}
e=e_{s,i}(T_{FP})= & b_{0}^{\prime}\exp(b_{1}\frac{(T_{0}-T_{FP})}{T_{0}T_{FP}}+b_{2}\ln(\frac{T_{FP}}{T_{0}})+b_{3}(T_{FP}-T_{0}))\label{eq:MK1-1}\end{eqnarray}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula \begin{equation}
e=e_{s,w}(T_{DP})=e_{0}\exp\left((\alpha-1)e_{6}+d_{2}(\frac{T_{0}-T_{DP}}{T_{DP}T_{0}}\right)+d_{3}\ln(\frac{T_{DP}}{T_{0}})+d_{4}(T_{DP}-T_{0})\label{eq:proposedNewWater}\end{equation}

\end_inset


