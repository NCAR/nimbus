	if [ "$$executeit" != "no" ]; then \
	echo $$imakeprefix$(IMAKE) -I$$imakeprefix$(IRULESRC) $(IMAKE_DEFINES) -DTOPDIR=$$imaketop -DCURDIR=$$curdir$$i; \
