#
# cshell startup script
#

unset noclobber

set clobber

setenv EDITOR	/bin/vi
setenv PRINTER	lp
setenv LPDEST	lp
setenv LESS	mCb50
setenv MORE	-c
setenv PAGER	less
setenv VISUAL	/bin/vi
 
setenv PATH	${PATH}:/jnet/local/winds/scripts

unsetenv LD_LIBRARY_PATH
 
# ADS stuff.
setenv DSM	$ADS/dsm
setenv DISC	$ADS/disc
setenv HEADER	$ADS/header
 
setenv WIND_BASE        $JLOCAL/vw
setenv WIND_HOST_TYPE   sun4-solaris2
setenv WIND_REGISTRY    mistral

# HIAPER SQL Stuff.
setenv PGDATA		/home/tmp/PostgreSQL
setenv PGHOST		hercules
setenv PGDATABASE	c130
setenv PGUSER		ads

#setenv MANPATH	$WIND_BASE/host/man:$WIND_BASE/target/man

if (`uname` == 'Linux') then
  alias cd 'set dot=$cwd; chdir \!* ; set prompt="`hostname -s`:$cwd \!\% "'
  set path = ( ${DISC}/bin $path )
  alias ls 'ls --color=tty'
else
  alias cd 'set dot=$cwd; chdir \!* ; set prompt="`uname -n`:$cwd \! % "'
  set path = ( ${DISC}/bin $path )
endif

alias ,		'set odot=$cwd; chdir $dot; set dot=$odot; pwd'
alias ..	'cd ..'
alias clr	'clear'
alias cls	'clear; ls'
alias cp	'cp -i'
alias dir	'ls -l \!* | more'
alias empty	'/usr/bin/cp /dev/null'
alias ff	'find . -name \!* -print'
alias ftp	'ftp -i'
alias gerp	'grep'
alias h		history
alias lman	'nroff -man \!* | $PAGER'
alias lookup	'grep \!* /usr/dict/words'
alias lpr4	'expand -4 \!* | lpr'
alias mail	'/usr/ucb/mail'
alias maek	'make'
alias mkae	'make'
alias mv	'mv -i'
alias psg	'ps -ax | grep \!*'
alias vh	'$PAGER /usr/include/\!*.h'
alias vhdr	'$PAGER $JLOCAL/include/header.h'
alias vi	vim
alias x		'chmod +x'
alias e		'xemacs -fg LightBlue -bg Maroon -geometry 90x46 &'

alias dat	'cd $DATA_DIR/\!*'
alias dev	'cd $LOCAL/dev/\!*'

alias mv162     'cd $WIND_BASE/target/config/mv162lc'
alias masp      'cd $WIND_BASE/target/config/masp'
