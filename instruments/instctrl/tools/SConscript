import sys

#env = Environment(tools = ['default', 'qt4', 'logx'])

tools = Split("""
default qt4 logx
""")

env = Environment(tools = ['default']+tools)

#Import('env')
#env = env.Clone(tools = ['default', 'qt4', 'logx'])

qtmods = ['QtSvg', 'QtCore', 'QtGui', 'QtNetwork', 'QtSql', 'QtOpenGL']
