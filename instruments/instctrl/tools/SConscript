sources = ['instrument.cpp', 'sendudp.cpp']
#acswsources = ['acswitch.cpp']

instrument = env.Program('instrument',sources)
#acswitch = env.Program('acswitch',acswsources)
#qtudp = env.Program('qtudp',sources)

env.Default(instrument)
