import eol_scons

env = Environment(tools=['default', 'osxqtapp', 'svninfo'])

apprevision = env['SVNREVISION']

proxyappname = 'Proxy-' + apprevision
proxyapp = env.OsxQtApp('.', '#/proxy/proxy', '#/Resources/proxy/proxyIcon.icns', proxyappname, apprevision)
Default(proxyapp)

switchappname = 'Switch-' + apprevision
switchapp = env.OsxQtApp('.', '#/switch/switch', '#/Resources/proxy/proxyIcon.icns', switchappname, apprevision)
Default(switchapp)
